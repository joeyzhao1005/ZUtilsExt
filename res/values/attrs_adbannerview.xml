<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ADBannerView">
        <attr name="style">
            <enum name="standard" value="0" />
            <enum name="tablet" value="1" />
            <enum name="cubein" value="2" />
            <enum name="cubeout" value="3" />
            <enum name="flipvertical" value="4" />
            <enum name="fliphorizontal" value="5" />
            <enum name="stack" value="6" />
            <enum name="zoomin" value="7" />
            <enum name="zoomout" value="8" />
            <enum name="rotateup" value="9" />
            <enum name="rotatedown" value="10" />
            <enum name="accordion" value="11" />
        </attr>
        <attr name="fadeEnabled" format="boolean" />
        <attr name="outlineEnabled" format="boolean" />
        <attr name="outlineColor" format="color|reference" />
        <attr name="ADBannerView_sidebuffer" format="integer" />
    </declare-styleable>
    <declare-styleable name="ADBannnerViewCircleIndicator">
        <attr name="ADBannnerViewCircleIndicator_activeColor" format="color" />
        <attr name="ADBannnerViewCircleIndicator_inactiveColor" format="color" />
        <attr name="ADBannnerViewCircleIndicator_radius" format="dimension" />
        <attr name="ADBannnerViewCircleIndicator_centered" format="boolean" />
        <attr name="ADBannnerViewCircleIndicator_useAnimation" format="boolean" />
        <!-- <attr name="ADBannnerViewCircleIndicator_fadeOut" format="integer" /> -->
        <attr name="ADBannnerViewCircleIndicator_inactiveType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="ADBannnerViewCircleIndicator_activeType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="ADBannnerViewCircleIndicator_circleSeparation" format="dimension" />
        <attr name="ADBannnerViewCircleIndicator_activeRadius" format="dimension" />
    </declare-styleable>

</resources>
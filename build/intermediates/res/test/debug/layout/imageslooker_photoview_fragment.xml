<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/black">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black">

        <com.kit.widget.imageview.photoview.PhotoView
            android:id="@+id/photoview"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"/>

        <com.kit.widget.imageview.photoview.GifView
            android:id="@+id/gifImageView"
            android:scaleType="fitCenter"
            android:adjustViewBounds="false"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:visibility="gone"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.kit.widget.progressbar.RoundProgressBar
                android:id="@+id/rpb"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_centerInParent="true"
                app:RoundProgressBar_roundColor="@color/dark_gray"
                app:RoundProgressBar_roundProgressColor="@color/holo_blue"
                app:RoundProgressBar_textColor="@color/holo_blue"
                app:RoundProgressBar_roundWidth="3dp"
                app:RoundProgressBar_textSize="18sp"/>
        </RelativeLayout>
    </FrameLayout>

</RelativeLayout><!-- From: file:/Users/Zhao/Documents/Doraemon/WithU/WithU/com.kit.extend.widget/res/layout/imageslooker_photoview_fragment.xml --><!-- From: file:/Users/Zhao/Documents/Doraemon/WithU/WithU/com.kit.extend.widget/build/intermediates/bundles/debug/res/layout/imageslooker_photoview_fragment.xml -->